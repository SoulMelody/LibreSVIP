msgid "Both note gap and punctuation"
msgstr "音符间隙和标点符号"

#: .srt.dump[1].choices[1].text
msgid "Note gap only"
msgstr "仅音符间隙"

#: .srt.dump[1].choices[2].text
msgid "Punctuation only"
msgstr "仅标点符号"

#: .srt.dump[2].title
msgid "Text encoding"
msgstr "文本编码"

#: .srt.dump[3].title
msgid "Track index"
msgstr "音轨序号"

#: .srt.dump[3].description
msgid "Start from 0, -1 means automatic selection"
msgstr "从0开始，-1表示自动选择"
